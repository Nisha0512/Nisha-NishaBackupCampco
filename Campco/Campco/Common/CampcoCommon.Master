<script>
    jQuery(document).ready(function () {
        jQuery('.infoicone').click(function () {
            jQuery('#menu-top-header').toggleClass('infodetails', 'slow', "easeOutSine");
        });

        var senseSpeed = 0;
        var previousScroll = 0.5;
        jQuery(window).scroll(function (event) {
            var scroller = jQuery(this).scrollTop();
            
            if (scroller - senseSpeed > previousScroll) {
                jQuery("div#header-top-qube").filter(':not(:animated)').slideUp();
                
            } else if (scroller + senseSpeed < previousScroll ) {
                jQuery("div#header-top-qube").filter(':not(:animated)').slideDown();
               
            }
            previousScroll = scroller;
           
        });
    });



</script>
