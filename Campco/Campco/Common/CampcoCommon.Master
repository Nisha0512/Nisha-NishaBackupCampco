            if (x == "3") {
                jQuery('tr.dropship').show();
            }
            else {
                jQuery('tr.dropship').hide();
            }
            if (cartCount == 0) {
                jQuery('a.checkout-button-header-cart').css("display", "none");
            }
            else {
                jQuery('a.checkout-button-header-cart').css("display", "inherit");
            }

