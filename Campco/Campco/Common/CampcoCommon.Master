<script type = "text/javascript">
    function ValidateEmail(email) {
        var expr = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
         return expr.test(email);     
    };
    function nospaces(t)
    {     
        return t.trim() == '' || t.trim().length == 0;
    };
    jQuery('.newsletterpopup1').click(function () {
        var sn = jQuery("#yourname1").val();
        var se = jQuery("#Email1").val();
            jQuery(".Namemsg").text("");
            jQuery(".Emailmsg").text("");
        console.log("chk : " + nospaces(jQuery("#yourname1").val()));
        if (nospaces(jQuery("#yourname1").val())) 
        {          
            jQuery(".Namemsg").text("Please enter valid name");  
            return;
         } 
        if (!ValidateEmail(jQuery("#Email1").val()))
        {                  
            jQuery(".Emailmsg").text("Please enter valid email");
            return;
        }
        usersubscribe(sn.trim(), se.trim());
        location.reload();
    });
</script>

        

    
